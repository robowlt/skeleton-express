{
  "authors": [
    {
      "homepage": "https://dl2.dev",
      "name": "DL2 IT Services, Ltd"
    },
    {
      "homepage": "https://github.com/douggr",
      "name": "Douglas Gontijo"
    }
  ],
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "express": "^5.0.0-alpha.7",
    "express-promise-router": "^3.0.3",
    "helmet": "^3.15.1",
    "knex": "^0.16.3",
    "morgan": "^1.9.1",
    "objection": "^1.6.3",
    "objection-db-errors": "^1.0.0",
    "objection-find": "^1.6.5",
    "pg": "^7.8.1"
  },
  "description": "Optionated skeleton for Express + Objection.js + Typescript apps",
  "devDependencies": {
    "@commitlint/cli": "^7.5",
    "@commitlint/config-angular": "^7.5",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.0.5",
    "@types/helmet": "^0.0.42",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.10.4",
    "@types/yargs": "^12.0.9",
    "concurrently": "^4.1",
    "eslint": "^5.15.1",
    "eslint-config-synacor": "^3.0.3",
    "fs-extra": "^7.0.1",
    "husky": "^1.3",
    "jest": "^24.1",
    "nodemon": "^1.18.10",
    "prettier": "^1.16",
    "stylelint": "^9.10",
    "stylelint-config-recommended-scss": "^3.2",
    "stylelint-order": "^2.1",
    "stylelint-scss": "^3.5",
    "supertest": "^3.4",
    "ts-jest": "^24.0",
    "ts-node": "^8.0",
    "tslint": "^5.13",
    "tslint-consistent-codestyle": "^1.15",
    "tslint-eslint-rules": "^5.4",
    "typescript": "^3.4.0-dev.20190302",
    "yargs": "^13.2.1"
  },
  "engines": {
    "node": "^9 || ^10"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -x @commitlint/config-angular -e",
      "pre-commit": "yarn lint"
    }
  },
  "license": "MIT",
  "name": "@dl2/skeleton-express",
  "scripts": {
    "build": "yarn ~copy-files && tsc",
    "build:fast": "yarn ~copy-files && tsc",
    "create:model": "ts-node tools/create-model",
    "format": "concurrently yarn:~prettier:* && concurrently 'yarn:~format:*'",
    "lint": "concurrently yarn:~lint:*",
    "postcreate:model": "yarn ~prettier:ts",
    "prebuild": "yarn test",
    "pretest": "yarn lint",
    "start": "nodemon -w src --ignore '*.spec.ts' -x ts-node -T --pretty src/index.ts",
    "test": "NODE_ENV=test jest",
    "~copy-files": "ts-node tools/copy-files",
    "~format:js": "yarn ~lint:js --fix",
    "~format:styles": "yarn ~lint:styles --fix",
    "~format:ts": "yarn ~lint:ts --fix",
    "~lint:js": "eslint --ignore-path .gitignore .",
    "~lint:styles": "stylelint 'src/**/*.scss'",
    "~lint:ts": "tslint -p tsconfig.json",
    "~prettier": "prettier --arrow-parens always --print-width 79 --trailing-comma all --write",
    "~prettier:others": "yarn ~prettier --single-quote '**/*.?(js|json)'",
    "~prettier:ts": "yarn ~prettier --parser typescript '**/*.ts'"
  },
  "version": "0.0.0"
}
